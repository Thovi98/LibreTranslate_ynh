{
    "name": "LibreTranslate",
    "id": "libretranslate",
    "packaging_format": 1,
    "description": {
        "en": "Open Source Machine Translation API, entirely self-hosted",
        "fr": "API de traduction automatique Open Source, entièrement auto-hébergée"
    },
    "version": "1.3.10~ynh1",
    "url": "https://libretranslate.com/",
    "upstream": {
        "license": "AGPL-3.0",
        "website": "https://libretranslate.com/",
        "demo": "https://libretranslate.com/",
        "admindoc": "https://libretranslate.com/docs/",
        "code": "https://github.com/LibreTranslate/LibreTranslate"
    },
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
